{
  "name": "react-audio-player-pro",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webbestmaster/react-audio-player-pro.git"
  },
  "bugs": {
    "url": "https://github.com/webbestmaster/react-audio-player-pro/issues"
  },
  "homepage": "https://github.com/webbestmaster/react-audio-player-pro#readme",
  "license": "MIT",
  "author": "",
  "version": "1.1.22",
  "description": "React Audio Player Pro",
  "keywords": [
    "react",
    "audio",
    "player",
    "react-audio-player",
    "react-audio-player-pro",
    "track list",
    "play list",
    "typescript",
    "ts",
    "flow",
    "flowjs"
  ],
  "main": "dist/index.js",
  "types": "dist/library.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development SIDE=front webpack-cli serve --progress",
    "start:transpile-only": "cross-env NODE_ENV=development SIDE=front TS_TRANSPILE_ONLY=true webpack-cli serve --progress",
    "tsc-watch": "tsc -p ./tsconfig.json --watch",
    "_server:start": "node ./dist-server/dist/index.js",
    "_server:build": "cross-env NODE_ENV=production SIDE=back IS_SERVER_PROD_BUILD=true NODE_OPTIONS=--max-old-space-size=1536 webpack --progress",
    "_server:watch:build": "cross-env NODE_ENV=production SIDE=back webpack --progress --watch",
    "_server:nodemon": "nodemon ./dist-server/dist/index.js",
    "build": "cross-env NODE_ENV=production SIDE=front NODE_OPTIONS=--max-old-space-size=1536 webpack --progress",
    "build-library": "cross-env NODE_ENV=production SIDE=front IS_BUILD_LIBRARY=true NODE_OPTIONS=--max-old-space-size=1536 webpack --progress && npm run tsc-library",
    "check": "npm run ts-coverage && npm run tsc && npm run test:unit && npm run eslint && npm run stylelint",
    "check:fix": "npm run prettier && npm run ts-coverage && npm run tsc && npm run test:unit && npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint ./**.scss",
    "stylelint:fix": "stylelint ./**.scss --fix",
    "prettier": "prettier --write \"./**/*.(js|ts|tsx)\" --loglevel warn",
    "tsc": "tsc -p ./tsconfig.json",
    "tsc-library": "tsc -p ./tsconfig.library.json && npm run tsc-library-css",
    "tsc-library-css": "cross-env NODE_ENV=production SIDE=front IS_BUILD_LIBRARY=true node ./webpack/util/library/css.js",
    "ts-coverage:front": "typescript-coverage-report --threshold=100 --strict=true --project ./www/",
    "ts-coverage:server": "typescript-coverage-report --threshold=100 --strict=true --project ./server/",
    "ts-coverage": "npm run ts-coverage:front && npm run ts-coverage:server",
    "test:e2e": "jest --config='test-e2e/config/jest.config.js' ./test-e2e/ --runInBand --silent --passWithNoTests",
    "test:unit": "jest --config='test-unit/config/jest.config.js' ./www/ --coverage --silent --passWithNoTests",
    "pre-commit-hook": "npm run check",
    "_postinstall": "cp ./git-hooks/pre-commit ./.git/hooks",
    "zip": "zip -9 --exclude=*node_modules* --exclude=*.git* -r my-project_`git symbolic-ref --short HEAD`-`date +%Y-%m-%d--%H-%M-%S` ./"
  },
  "peerDependencies": {
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "markdown-pro": "^1.0.25",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-transform-strict-mode": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "css-modules-typescript-loader": "^4.0.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^8.17.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "jest-transform-stub": "^2.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "^2.6.2",
    "raw-loader": "^4.0.2",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "style-loader": "^3.3.1",
    "stylelint": "^14.9.1",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-scss": "^4.2.0",
    "terser-webpack-plugin": "^5.3.3",
    "ts-jest": "^28.0.4",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.3",
    "typescript-coverage-report": "^0.6.4",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  }
}
